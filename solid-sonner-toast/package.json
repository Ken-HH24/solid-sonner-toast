{
    "name": "solid-sonner-toast",
    "type": "module",
    "version": "0.0.1",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "browser": {},
    "typesVersions": {},
    "exports": {
        "import": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "types": "./dist/index.d.ts",
    "scripts": {
        "build": "tsup",
        "dev": "tsup src/index.tsx --watch"
    },
    "tsup": {
        "entry": [
            "src/index.ts"
        ],
        "splitting": false,
        "sourcemap": true,
        "clean": true
    },
    "files": [
        "dist/"
    ],
    "publishConfig": {
        "types": "dist/index.d.ts",
        "access": "public"
    },
    "peerDependencies": {
        "solid-js": "^1.5.0"
    },
    "dependencies": {
        "solid-js": "^1.8.7"
    },
    "devDependencies": {
        "tsup": "^8.0.2",
        "tsup-preset-solid": "^2.2.0",
        "typescript": "^5.2.2"
    }
}
